`timescale 1ns / 1ps

module testgateand; // use your desired testbench name 

	// Inputs
	reg a;
	reg b;

	// Outputs
	wire y;
	// Instantiate the Unit Under Test (UUT)
	//here use the design files name before uut
  gatenand uut (
		.a(a), 
		.b(b), 
		.y(y)
	);

	initial begin
		// Set up VCD file for waveform dumping
		$dumpfile("dump.vcd");
		$dumpvars;

		a = 0;
		b = 0;
		#100;
		
		a = 0;
		b = 1;
		#100;
		
		a = 1;
		b = 0;
		#100;
		
		a = 1;
		b = 1;
		#100;
	end
      
endmodule
